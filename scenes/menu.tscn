resource_name = "menu"
script/source = "extends Control

#--- Signals ---
#--- Enums ---
#--- Constants ---
#--- Exported Variables ---
#--- Public Variables ---
#--- Private Variables ---
#--- Onready Variables ---
#--- Engine Callbacks ---
func _ready() -> void:
	pass


#--- Public Functions ---
#--- Private Functions ---
# - Common Use Functions -
# - Setget Functions -
# - Signal Functions -

# start button functions
func _on_button_mouse_entered():
	$start_button/AnimationPlayer.play(\"hover\")

func _on_button_mouse_exited():
	$start_button/AnimationPlayer.play(\"normal\")

func _on_button_pressed():
	get_tree().change_scene_to_file(\"res://scenes/battle_table.tscn\")

# options button functions
func _on_options_button_mouse_entered():
	$options_button/AnimationPlayer.play(\"hover\")

func _on_options_button_mouse_exited():
	$options_button/AnimationPlayer.play(\"normal\")

func _on_options_button_pressed():
	$MenuOpcoes.visible = true
	$start_button.disabled = true
	$options_button.disabled = true
	$close_game_button.disabled = true
	
	
# close game button functions
func _on_close_game_button_mouse_entered():
	$close_game_button/AnimationPlayer.play(\"hover\")

func _on_close_game_button_mouse_exited():
	$close_game_button/AnimationPlayer.play(\"normal\")

func _on_close_game_button_pressed():
	$MenuConfirmarSaida.visible = true
	$start_button.disabled = true
	$options_button.disabled = true
	$close_game_button.disabled = true

	
func _on_sair_sim_mouse_entered():
	$MenuConfirmarSaida/sair_sim/AnimationPlayer.play(\"hover\")
	$MenuConfirmarSaida/capivara_menu_sair/capivara_normal.stop()
	$MenuConfirmarSaida/capivara_menu_sair/capivara_feliz.stop()	
	$MenuConfirmarSaida/capivara_menu_sair/capivara_triste.play(\"capivara_triste\")	
	
func _on_sair_sim_mouse_exited():
	$MenuConfirmarSaida/sair_sim/AnimationPlayer.play(\"normal\")
	$MenuConfirmarSaida/capivara_menu_sair/capivara_triste.stop()	
	$MenuConfirmarSaida/capivara_menu_sair/capivara_feliz.stop()
	$MenuConfirmarSaida/capivara_menu_sair/capivara_normal.play(\"capivara_normal\")
	
func _on_sair_sim_pressed():
	get_tree().quit()

func _on_sair_nao_mouse_entered():
	$MenuConfirmarSaida/sair_nao/AnimationPlayer.play(\"hover\")
	$MenuConfirmarSaida/capivara_menu_sair/capivara_normal.stop()
	$MenuConfirmarSaida/capivara_menu_sair/capivara_triste.stop()	
	$MenuConfirmarSaida/capivara_menu_sair/capivara_feliz.play(\"capivara_feliz\")

func _on_sair_nao_mouse_exited():
	$MenuConfirmarSaida/sair_nao/AnimationPlayer.play(\"normal\")
	$MenuConfirmarSaida/capivara_menu_sair/capivara_triste.stop()	
	$MenuConfirmarSaida/capivara_menu_sair/capivara_feliz.stop()
	$MenuConfirmarSaida/capivara_menu_sair/capivara_normal.play(\"capivara_normal\")

func _on_sair_nao_pressed():
	$MenuConfirmarSaida.visible = false
	$MenuConfirmarSaida/sair_nao/AnimationPlayer.play(\"normal\")
	$start_button.disabled = false
	$options_button.disabled = false
	$close_game_button.disabled = false

# voltar para o menu
func _on_button_voltar_pressed():
	$MenuOpcoes.visible = false
	$start_button.disabled = false
	$options_button.disabled = false
	$close_game_button.disabled = false

func _on_button_voltar_mouse_entered():
	$MenuOpcoes/button_voltar/AnimationPlayer.play(\"hover\")

func _on_button_voltar_mouse_exited():
	$MenuOpcoes/button_voltar/AnimationPlayer.play(\"normal\")
"

